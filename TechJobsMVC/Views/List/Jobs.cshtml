
<div class="container body-content">


    <h1>@ViewBag.title</h1>


    <ul>

        @foreach (var job in ViewBag.jobs)
        {

            <li>ID: @job.Id</li>
            <li>Name: @job.Name</li>
            <li>Employer: @job.Employer</li>
            <li>Location: @job.Location</li>
            <li>Position Type: @job.CoreCompetency</li>
            <br />

        }

    </ul>

</div>